{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@now/node"
    },
    { 
      "src": "/src/swagger/**", 
      "use": "@now/node" 
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    },
    { 
      "src": "/dist/(.*)", 
      "dest": "/dist/index.js"
    }
  ]
}